<!--  Minimal Tabs solutions from http://www.red-team-design.com/google-play-minimal-tabs-with-css3-jquery   -->
<script type="text/javascript">

	function resetTabs() {
		$("#content > div").hide();
		$("#tabs a").attr("id", "");
		HideHeaders();
	}
	var myUrl = window.location.href;
	var showAll = false;
	var picTitle;
	var myUrlTab = myUrl.substring(myUrl.indexOf("#"));
	var myUrlTabName = myUrlTab.substring(0, 4);
	(function() {
		$("#content > div").hide();
		$("#tabs li:first a").attr("id", "current");
		$("#content > div:first").fadeIn();
		$("#tabs a").on("click", function(e) {
			e.preventDefault();
			if ($(this).attr("id") == "current") {
				return
			} else {
				resetTabs();
				$(this).attr("id", "current");
				$($(this).attr("name")).fadeIn()
			}
		});
		$("#mainPic a").on("click", function(e) {
			if(!showAll){
				myUrlTab = "#tabImages";
				resetTabs();
				$("a[name='" + myUrlTab + "']").attr("id", "current");
				$(myUrlTab).fadeIn();
			}
		});
		for (i = 1; i <= $("#tabs li").length; i++) {
			if (myUrlTab == myUrlTabName + i) {
				resetTabs();
				$("a[name='" + myUrlTab + "']").attr("id", "current");
				$(myUrlTab).fadeIn()
			}
		}
	})();
	
	function HideHeaders(){		
		$(".subHeading").hide();
	}
	
	function ShowAll(){		
		showAll = true;
		$("#tabs").find("li").each(function(){
			var name = $(this).children("a").attr("name");
			var tab = $("#content").find(name);
			tab.find(".subHeading").show();
			tab.fadeIn();
		});
		
		$("#mainPic a").bind('click', false);
		picTitle = $("#mainPic a").attr("title");
		$("#mainPic a").attr("title", "");
		$("#mainPic a").removeAttr("href");
		
		$("#tabs").hide();
		$("#ShowAllButton").hide();
		$("#ShowTabsButton").fadeIn();
 	}
	
	function ShowTabs(){
		showAll = false;
		$("#tabs").find("li").each(function(){
			var name = $(this).children("a").attr("name");
			var tab = $("#content").find(name);
			tab.find(".subHeading").hide();
			tab.hide();
		});
		
		$("#mainPic a").bind('click', true);		
		$("#mainPic a").attr("title", picTitle);
		$("#mainPic a").attr("href", "#");
		
		$("#tabs").fadeIn();
		$("#ShowAllButton").fadeIn();
		$("#ShowTabsButton").hide();
		resetTabs();
		$("#content > div:first").fadeIn();
	}
</script>

<!-- Mapping JS -------------------------------------------------------------------------------------------------------------------------------------------- -->

<script type="text/javascript">
    var djConfig = {parseOnLoad: true};
</script>
<script type="text/javascript"
	    src="https://js.arcgis.com/3.13/"></script>

<script type="text/javascript">
    
    // init dojo requirements
    dojo.require("dijit.layout.BorderContainer");
    dojo.require("dijit.layout.ContentPane");
    dojo.require('dijit.layout.TabContainer');
    dojo.require("esri.map");
    dojo.require("dijit.Dialog");
    dojo.require("esri.dijit.Legend");
	dojo.require("dojo.ready");

    // create map var
    var map;

    function init() {
    	
         
        // init esri config defaults for io (proxyUrl = 'mapping', always use proxy = 'true')
    	esri.config.defaults.io.proxyUrl = "mapping";
        // By making the following "false" you jump around MappingProxyController.class, which
        // proxies thru the loadbalancer (plants.usda.gov or plants-dev.dev.sc....) in order to get
        // around firewall problems et al wrt the geoserver being in a different domain/subnet.
        // Making this false will only work if the geoserver is accessible from the actual host.
        // esri.config.defaults.io.alwaysUseProxy = true;
    	esri.config.defaults.io.alwaysUseProxy = true;
        

		queryString = dojo.queryToObject(window.location.search);

        plantSymbol = document.getElementById('vSymbol').value;
		if (! plantSymbol ||  plantSymbol.length < 1) {
			plantSymbol = queryString["?symbol"];
			if (! plantSymbol ||  plantSymbol.length < 1) {
				plantSymbol = queryString["?keywordquery"];
				if (! plantSymbol ||  plantSymbol.length < 1) {
					plantSymbol = "Plant not selected";
				}
			}
		}

        // init lods
		var lods = [
		  {
          "level": 3,
          "resolution": 19567.8792409999,
          "scale": 73957190.948944},
          {
          "level": 4,
          "resolution": 9783.93962049996,
          "scale": 36978595.474472},
          {
          "level": 5,
          "resolution": 4891.96981024998,
          "scale": 18489297.737236 },
          {
          "level": 6,
          "resolution": 2445.98490512499,
          "scale": 9244648.868618 },
          {
          "level": 7,
          "resolution": 1222.99245256249,
          "scale": 4622324.434309},
		  {
          "level": 8,
          "resolution": 611.49622628138,
          "scale": 2311162.217155 },
  		  {
          "level": 9,
          "resolution": 305.748113140558,
          "scale": 1155581.108577 },
		  {
          "level": 10,
          "resolution": 152.874056570411,
          "scale": 577790.554289 },
		  {
          "level": 11,
          "resolution": 76.4370282850732,
          "scale": 288895.277144}
        ];

        // init page title
		var pageTitleValue = '<span style="font-size:125%;color:#000000;">Symbol: ' + plantSymbol + '</span>';
		dojo.html.set(dojo.byId("pageTitle"), pageTitleValue);

        // create new map
        var initExtent = new esri.geometry.Extent({"xmin":-177,"ymin":3,"xmax":-20,"ymax":72,"spatialReference":{"wkid":4326}});
//      esri.config.defaults.map.slider = { left:"40px", top:"100px", width:null, height:"300px" };

        // Note: map works only with Internet Explorer, not Firefox or Chrome
        map = new esri.Map("map",{ 
            sliderStyle: "large",
            wrapAround180:true,
            extent:esri.geometry.geographicToWebMercator(initExtent),            
            lods: lods});


        
        // To get a nice map, colored with terrain, no state boundaries or legend-ish shadings
        // uncomment next four lines to show terrain in base map
//        var GisServer = "http://geodatacache-test.dev.sc.egov.usda.gov"
//                + "/arcgis/rest/services/topographic_images/usa_topo_maps/MapServer";
//        var basemap = new esri.layers.ArcGISTiledMapServiceLayer(GisServer);
        // map.addLayer(basemap);


        // NOTE: layer definitions are modified by the proxy
		var layerDefinitions = [];
        layerDefinitions[2] = "SYMBOL=." + plantSymbol + ".";
        layerDefinitions[3] = "SYMBOL=." + plantSymbol + ".";
        layerDefinitions[4] = "SYMBOL=." + plantSymbol + ".";
        layerDefinitions[5] = "SYMBOL=." + plantSymbol + ".";
        layerDefinitions[6] = "SYMBOL=." + plantSymbol + ".";

        GisServer = "GisServer";
        // make the map (proxy?) call to the loadbalancer and get back a dynamic map
        var dynamicmap = new esri.layers.ArcGISDynamicMapServiceLayer(GisServer);
	
		dynamicmap.setLayerDefinitions(layerDefinitions);
		
        // add dynamicmap returned by proxy to the map object - will hit the proxy a 2nd time
		map.addLayer(dynamicmap);

		dojo.connect(map,'onLayersAddResult',function(results) {

		    var layerInfo = dojo.map(results, function(layer,index) {
		    	                return {layer:layer.layer,title:layer.layer.name};
		    	            });
		    
	        if (layerInfo.length > 0) {
		        var legendDijit = new esri.dijit.Legend({
		        	                  map:map,
		        	                  layerInfos:layerInfo
		        	              }, "legendDiv");
		        legendDijit.startup();
		    }
		});

        dojo.connect(map, 'onLoad', function(theMap) {
        	//resize the map when the browser resizes
            dojo.connect(dijit.byId('map'), 'resize', map,map.resize);
        });
        
    }

    dojo.addOnLoad(init);
 
</script>

<script>
	jQuery(document).ready(function ($) {
			
    	var options = {
        	$AutoPlay: false,                                    //[Optional] Whether to auto play, to enable slideshow, this option must be set to true, default value is false
            $AutoPlayInterval: 4000,                            //[Optional] Interval (in milliseconds) to go for next slide since the previous stopped if the slider is auto playing, default value is 3000
            $SlideDuration: 500,                                //[Optional] Specifies default duration (swipe) for slide in milliseconds, default value is 500
            $DragOrientation: 3,                                //[Optional] Orientation to drag slide, 0 no drag, 1 horizental, 2 vertical, 3 either, default value is 1 (Note that the $DragOrientation should be the same as $PlayOrientation when $DisplayPieces is greater than 1, or parking position is not 0)
    	
            $ThumbnailNavigatorOptions: {
            	$Class: $JssorThumbnailNavigator$,              //[Required] Class to create thumbnail navigator instance
                $ChanceToShow: 2,                               //[Required] 0 Never, 1 Mouse Over, 2 Always

                $AutoCenter: 3,
                $Loop: 0,                                       //[Optional] Enable loop(circular) of carousel or not, 0: stop, 1: loop, 2 rewind, default value is 1
                $SpacingX: 3,                                   //[Optional] Horizontal space between each thumbnail in pixel, default value is 0
                $SpacingY: 6,                                   //[Optional] Vertical space between each thumbnail in pixel, default value is 0
                $DisplayPieces: 6,                              //[Optional] Number of pieces to display, default value is 1
                $ParkingPosition: 204,                          //[Optional] The offset position to park thumbnail,

                $ArrowNavigatorOptions: {
                	$Class: $JssorArrowNavigator$,              //[Requried] Class to create arrow navigator instance
                    $ChanceToShow: 2,                               //[Required] 0 Never, 1 Mouse Over, 2 Always
                    $AutoCenter: 2,                                 //[Optional] Auto center arrows in parent container, 0 No, 1 Horizontal, 2 Vertical, 3 Both, default value is 0
                    $Steps: 6                                       //[Optional] Steps to go for each navigation request, default value is 1
                }
         	}
     	};

     	var jssor_slider1 = new $JssorSlider$("slider1_container", options);

            //responsive code begin
            //you can remove responsive code if you don't want the slider scales while window resizes
        function ScaleSlider() {
        	var parentWidth = jssor_slider1.$Elmt.parentNode.clientWidth;
            if (parentWidth)
            	jssor_slider1.$SetScaleWidth(Math.min(parentWidth, 500));
            else
           		window.setTimeout(ScaleSlider, 30);
        }

    	ScaleSlider();

	});
</script>
